    } </span><span><span class="hljs-keyword">else</span></span><span> {
        </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(</span><span><span class="hljs-string">`âŒ ERROR: Jumpscare file <span class="hljs-subst">${scareId}</span></span></span><span> missing!`);
