</span><span><span class="hljs-comment">// ðŸš€ UPDATE PLAYER HEALTH</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">updateHealth</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"health"</span></span><span>).</span><span><span class="hljs-property">textContent</span></span><span> = playerHealth;
    </span><span><span class="hljs-keyword">if</span></span><span> (playerHealth &lt;= </span><span><span class="hljs-number">0</span></span><span>) {
