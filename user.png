</span><span><span class="hljs-comment">// âœ… SURVIVAL MECHANICS: HEALING &amp; DEFENSES</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">useItem</span></span><span>(</span><span><span class="hljs-params">item</span></span><span>) {
