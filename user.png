    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"startButton"</span></span><span>).</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">display</span></span><span> = </span><span><span class="hljs-string">"none"</span></span><span>;
