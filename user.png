}

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">loseGame</span></span><span>(</span><span><span class="hljs-params">message</span></span><span>) {
    </span><span><span class="hljs-title function_">printToTerminal</span></span><span>(</span><span><span class="hljs-string">`[ERROR]: <span class="hljs-subst">${message}</span></span></span><span>`);
    </span><span><span class="hljs-built_in">setTimeout</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {
        </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-property">body</span></span><span>.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">`&lt;h1 class='glitch'&gt;GAME OVER&lt;/h1&gt;&lt;p&gt;<span class="hljs-subst">${message}</span></span></span><span>&lt;/p&gt;`;
    }, </span><span><span class="hljs-number">2000</span></span><span>);
}

</span><span><span class="hljs-comment">// âœ… SURVIVAL MECHANICS: HEALING &amp; DEFENSES</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">useItem</span></span><span>(</span><span><span class="hljs-params">item</span></span><span>) {
    </span><span><span class="hljs-keyword">if</span></span><span> (inventory[item] &gt; </span><span><span class="hljs-number">0</span></span><span>) {
        </span><span><span class="hljs-keyword">if</span></span><span> (item === </span><span><span class="hljs-string">"backupFile"</span></span><span>) {
            playerHealth = </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">min</span></span><span>(</span><span><span class="hljs-number">5</span></span><span>, playerHealth + </span><span><span class="hljs-number">1</span></span><span>);
            </span><span><span class="hljs-title function_">printToTerminal</span></span><span>(</span><span><span class="hljs-string">"[SYSTEM]: Backup File Restored. +1 HP"</span></span><span>);
        } </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-keyword">if</span></span><span> (item === </span><span><span class="hljs-string">"firewallShield"</span></span><span>) {
            </span><span><span class="hljs-title function_">printToTerminal</span></span><span>(</span><span><span class="hljs-string">"[SYSTEM]: Firewall Active. Next attack blocked."</span></span><span>);
            inventory.</span><span><span class="hljs-property">firewallShield</span></span><span>--;
        } </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-keyword">if</span></span><span> (item === </span><span><span class="hljs-string">"purgeCommand"</span></span><span>) {
            corruption = </span><span><span class="hljs-number">0</span></span><span>;
            playerHealth--;
            </span><span><span class="hljs-title function_">printToTerminal</span></span><span>(</span><span><span class="hljs-string">"[SYSTEM]: Purge Complete. Corruption Reset. -1 HP"</span></span><span>);
        }
        inventory[item]--;
        </span><span><span class="hljs-title function_">updateStats</span></span><span>();
    } </span><span><span class="hljs-keyword">else</span></span><span> {
        </span><span><span class="hljs-title function_">printToTerminal</span></span><span>(</span><span><span class="hljs-string">"[ERROR]: No more of that item left."</span></span><span>);
    }
}

