    } </span><span><span class="hljs-keyword">else</span></span><span> {
        </span><span><span class="hljs-title function_">allowNextMove</span></span><span>();
    }
}

</span><span><span class="hljs-comment">// ðŸ–¥ PRINT TO TERMINAL</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">printToTerminal</span></span><span>(</span><span><span class="hljs-params">text, isGlitch = <span class="hljs-literal">false</span></span></span><span>) {
    </span><span><span class="hljs-keyword">let</span></span><span> terminal = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"terminal"</span></span><span>);
    terminal.</span><span><span class="hljs-property">value</span></span><span> += (isGlitch ? </span><span><span class="hljs-string">"[ERROR] "</span></span><span> : </span><span><span class="hljs-string">""</span></span><span>) + text + </span><span><span class="hljs-string">"\n"</span></span><span>;
    terminal.</span><span><span class="hljs-property">scrollTop</span></span><span> = terminal.</span><span><span class="hljs-property">scrollHeight</span></span><span>;
}

</span><span><span class="hljs-comment">// ðŸ›  START GAME</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">startGame</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
