    </span><span><span class="hljs-built_in">setTimeout</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {
        </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-property">body</span></span><span>.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">`&lt;h1 class='glitch'&gt;YOU ESCAPED&lt;/h1&gt;&lt;p&gt;<span class="hljs-subst">${message}</span></span></span><span>&lt;/p&gt;`;
