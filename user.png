        </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-property">body</span></span><span>.</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">backgroundColor</span></span><span> = </span><span><span class="hljs-string">"red"</span></span><span>;
