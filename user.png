</span><span><span class="hljs-comment">// ðŸš€ UPDATE PLAYER HEALTH, CORRUPTION &amp; CHECK WIN/LOSS</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">updateStats</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
